[Unit]
Description=gofastdfs service 
Wants=network.target 

[Service]
PIDFile=/home/gofastdfs/conf/app.pid
Environment="GO_FASTDFS_DIR=E:/go_space/src/go-fastdfs" #/home/gofastdfs 修改成你的安装路径
ExecStart=E:/go_space/src/go-fastdfs/fileserver $GO_FASTDFS_DIR
ExecReload=/bin/kill -s HUP $MAINPID
ExecStop=/bin/kill -s QUIT $MAINPID
PrivateTmp=true
Restart=always

[Install] 
WantedBy=multi-user.target